#!/usr/bin/env bash
#
#
THE_ARGS="$@"
THIS_DIR="$(dirname "$(dirname "$(realpath "$0")")")"
PATH="$PATH:$THIS_DIR/../sh_string/bin"

if [[ -z "$@" ]]; then
  action="watch"
else
  action=$1
  shift
fi

set -u -e -o pipefail
PATH="$PATH:$THIS_DIR/../mksh_setup/bin"

    source "$THIS_DIR/../mksh_setup/bin/source/name-to-func/_.sh"
    name-to-func "$action"  "$@"
